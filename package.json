{
  "name": "jcinkcollection",
  "version": "1.0.0",
  "description": "Collection of scripts/plugins made for JCINK, a forum host service.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "compile:sass": "sass --no-source-map style/src:style/css --style compressed",
    "concat:sass": "node utils/concat style/css/*.css style/css/index.css",
    "prefix:css": "postcss --no-map autoprefixer style/css/index.css -o style/css/index.css",
    "build": "npm run compile:sass && npm run concat:sass && npm run prefix:css",
    "dev": "sass --watch style/src:style/css"
  },
  "author": "tohmatosauce",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "glob": "^11.0.2",
    "postcss": "^8.5.4",
    "postcss-cli": "^11.0.1",
    "postcss-preset-env": "^10.2.2"
  },
  "type": "module"
}
